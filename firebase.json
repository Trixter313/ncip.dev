{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "rollup.config.mjs",
      "package.json",
      "package-lock.json",
      "web-dev-server.config.mjs",
      "**/.*",
      "**/node_modules/**",
      "**/src",
      "**/dist",
      "**/archive",
      "**/deploy.*"
    ],
    "headers": [{
      "source" : "**",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    }]
  }
}
